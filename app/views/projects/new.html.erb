<%= form_for :project , url: projects_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :shortDesc %><br>
    <%= f.text_area :shortDesc %>
  </p>
  
  <p>
    <%= f.label :longDesc %><br>
    <%= f.text_area :longDesc %>
  </p>

  <p>
    <%= f.label :imgName %><br>
    <%= f.text_area :imgName %>
  </p>
  <p>
    <%= f.label :tag_names %>
    <%=f.text_field :tag_names %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>