<%= form_for :project , url: projects_path , :html => { :multipart => true } do |f| %>
  <p>
    <%= f.label "Project Name" %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label "Short Description" %><br>
    <%= f.text_area :shortDesc %>
  </p>
  
  <p>
    <%= f.label "Long description" %><br>
    <%= f.text_area :longDesc %>
  </p>

  <p>
    <%= f.label "Alternative text" %><br>
    <%= f.text_area :imgName %>
  </p>
  <p>
    <%= f.label :tag_names %>
    <%=f.text_field :tag_names %>
  </p>
  <p>
   <%= f.file_field :image %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>